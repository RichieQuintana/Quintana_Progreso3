<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Quintana_Progreso3.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Quintana_Progreso3.ViewModel"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem Title="Home" FlyoutDisplayOptions="AsSingleItem">
        <ShellContent ContentTemplate="{DataTemplate local:RicardoQuintanaMeal}" />
    </FlyoutItem>
    <FlyoutItem Title="List" FlyoutDisplayOptions="AsSingleItem">
        <ShellContent ContentTemplate="{DataTemplate local:RicardoQuintanaList}" />
    </FlyoutItem>
    
</Shell>
